[
  {
    "id": "email-regex",
    "label": "EMAIL",
    "regex": true,
    "patterns": ["(?i)[a-z0-9\\.\\-+_]+@[a-z0-9\\.\\-+_]+\\.[a-z]+"]
  },
  {
    "id": "ipv4-regex",
    "label": "IP_ADDRESS",
    "regex": true,
    "patterns": [
      "(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)(?:\\.(?:25[0-5]|2[0-4]\\d|1\\d{2}|[1-9]?\\d)){3}"
    ]
  }
]
